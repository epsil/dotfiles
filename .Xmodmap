!Mac-like Fn bindings for the ThinkPad T500
!xmodmap ~/.Xmodmap

!keycode 151 = XF86WakeUp
!keycode 151 = Delete

!keycode 171 = XF86AudioNext
keycode 171 = End
!keycode 172 = XF86AudioPlay
keycode 172 = Next
!keycode 173 = XF86AudioPrev
keycode 173 = Home
!keycode 174 = XF86AudioStop
keycode 174 = Prior
